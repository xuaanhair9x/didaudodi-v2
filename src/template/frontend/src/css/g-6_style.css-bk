body {
    font-family: Open Sans;
    font-size: 14px;
    position: relative;
}
body.overlay::after {
    content: '';
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgb(0 0 0 / 70%);
    z-index: 1029;
}
.c-modal::-webkit-scrollbar {
    width: 0;
}

/* Style chung b-search */
[class*="b-search-"] {
    color: #666;
    font-size: 13px;
    max-width: 400px;
    background-color: #fff;
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: stretch;
    align-items: stretch;
}
[class*="b-search-"]>input {
    width: 100%;
    color: inherit;
    font-size: 1em;
    padding: 0 5px 0 7px;
    line-height: normal;
    border: none;
    background-color: transparent;
}
[class*="b-search-"]>input::-webkit-input-placeholder {
    color: #999;
}
[class*="b-search-"]>input::-moz-placeholder {
    color: #999;
}
[class*="b-search-"]>input:-moz-placeholder {
    color: #999;
}
[class*="b-search-"]>input:-ms-input-placeholder {
    color: #999;
}
[class*="b-search-"]>input:focus {
    outline: none;
}
[class*="b-search-"]>button {
    -webkit-flex: none;
    -ms-flex: none;
    flex: none;
    font-size: 1em;
    box-shadow: none;
    line-height: 0;
}
[class*="b-search-"]>button i {
    line-height: inherit;
}

/* BEGIN b-search-2 */
.b-search-2 {
    border: 1.5px solid #00513F;
    width: 400px;
    border-radius: 5px;
}
.b-search-2>button {
    border-radius: 5px;
}
.b-search-2>button[class] {
    font-size: 14px;
    padding: 1em 0.584em 1em 0.584em;
    margin: 2px 2px 2px 0;
    background-color: #04727f;
}
.b-search-2>button:not([class]) {
    color: inherit;
    padding: 1.077em 0.616em 1.077em 0.616em;
    background-color: transparent;
}
.b-search-2>div {
    position: relative;
}
.b-search-2>div>ul {
    display: none;
    overflow: auto;
    border: 1px solid #ddd;
    background-color: #fff;
    border-top: 3px solid #bbb;
    z-index: 111;
    padding: 5px 10px;
    min-width: 110px;
    position: absolute;
    top: 100%;
    line-height: normal;
    left: 0;
    border-radius: 3px;
}
.b-search-2>div>ul li {
    padding: 5px 0;
    -webkit-transition: color .4s ease;
    transition: color .4s ease;
}
.b-search-2>div>ul li:not(:last-child) {
    border-bottom: 1px dashed #eee;
}
.b-search-2>div>ul li label {
    display: block;
    cursor: pointer;
    position: relative;
}
.b-search-2>div>ul li label input {
    position: absolute;
    opacity: 0;
}
.b-search-2>div>ul li.is-focus, .b-search-2>div>ul li:hover {
    color: #00513F;
}
.b-search-2>div>span {
    display: block;
    max-width: 170px;
    min-width: 100px;
    padding: 0 25px 0 10px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    cursor: pointer;
    position: relative;
    line-height: normal;
}
.b-search-2>div>span::after {
    content: '';
    width: 1px;
    height: 100%;
    background-color: #ddd;
    position: absolute;
    left: 0;
}
.b-search-2>div>span::before {
    content: '\f107';
    font-family: FontAwesome;
    position: absolute;
    top: 3px;
    right: 10px;
}
/* END b-search-2 */
/* Style chung s-header */
.hd-logo {
    line-height: 0;
}

.hd-menu h2 {
    font-size: inherit;
    font-weight: inherit;
}

a[ht-trigger="hd-menu"] {
    display: none;
    font-size: 14px;
    /* line-height: 1; */
    /* padding: 0.5em; */
    /* border: 1px solid; */
}
a[ht-trigger="hd-menu"] i:first-child {
    float: left;
}
a[ht-trigger="hd-menu"] i:last-child {
    float: right;
}
@keyframes slide-up {
    0% {
        opacity: 0;
        transform: translateX(200px);
    }
    100% {
        opacity: 1;
        transform: translateX(0%);
    }
}
@keyframes slide-up-2 {
    0% {
        opacity: 0;
        transform: translateY(200px);
    }
    100% {
        opacity: 1;
        transform: translateX(0%);
    }
}
