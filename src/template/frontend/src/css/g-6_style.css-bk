.b-card-53 {
    display: block;
    margin-bottom: 30px;
    height: calc(100% - 30px);
    box-shadow: rgba(0, 0, 0, 0.16) 0px 1px 4px;
    border-radius: 10px;
}
.b-card-53 .b_thumb {
    border-radius: 10px;
    overflow: hidden;
}
.b-card-53 .b_thumb:before {
    position: inherit;
    top: 50%;
    left: 50%;
    content: "";
    transform: translateX(-50%);
    width: 25%;
    height: 25%;
    opacity: 0;
    transition: all 0.25s ease;
    background-color: rgba(25, 48, 89, 0.3);
    z-index: 99;
}
.b-card-53:hover .b_thumb:before {
    width: 100%;
    height: 100%;
    opacity: 1;
}
.b-card-53 .b_thumb:after {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-top: 1px solid #fff;
    border-bottom: 1px solid #fff;
    content: "";
    opacity: 0;
    -webkit-transform: rotate3d(0, 0, 1, 45deg) scale3d(1, 0, 1);
    transform: rotate3d(0, 0, 1, 45deg) scale3d(1, 0, 1);
    -webkit-transform-origin: 50% 50%;
    transform-origin: 50% 50%;
    transition: opacity 0.5s, transform 0.5s;
    z-index: 99;
}
.b-card-53:hover .b_thumb:after {
    opacity: 1;
    -webkit-transform: rotate3d(0, 0, 1, 45deg) scale3d(1, 1, 1);
    transform: rotate3d(0, 0, 1, 45deg) scale3d(1, 1, 1);
}

.b-card-53 .b_title {
    font-size: 18px;
    font-weight: bold;
    margin: 5px 0 10px;
    -webkit-transition: 0.3s color ease-in-out;
    -o-transition: 0.3s color ease-in-out;
    transition: 0.3s color ease-in-out;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}
.b-card-53:hover .b_title {
    color: #00513F;
}
.b-card-53 .b_content {
    padding: 15px;
}
